                            .orders__form-content
                                .orders__infos  
                                    p.orders__info Адрес доставка
                                    input(type="text" placeholder="Адрес доставка" required).orders__input
                                .orders__infos  
                                    p.orders__info Имя
                                    input(type="text" placeholder="Имя" required).orders__input
                                .orders__infos  
                                    p.orders__info Телефон
                                    input(type="number" placeholder="Телефон" required).orders__input
                                .orders__row 
